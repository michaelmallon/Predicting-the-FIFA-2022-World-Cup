   "outputs": [],
   "source": [
    "joined = pd.merge(left = players, right = valuations, how = 'inner', on = 'player_id' )\n",
    "#firsttierplayersonly (revise later if no players)\n",
    "joined = pd.merge(left = joined, right = clubs, how = 'inner', on = 'club_id' )\n",
    "joined = pd.merge(left = joined, right = competitions, how = 'inner', left_on='player_club_domestic_competition_id', right_on='competition_id' )"
   ]
